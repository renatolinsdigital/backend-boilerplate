{
  "name": "backend-nestjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "clean": "node scripts/clean.js",
    "free": "node scripts/free-port.js",
    "dev": "npm run free && ts-node-dev --respawn --transpile-only src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"{src,scripts}/**/*.{js,ts}\"",
    "lint:fix": "eslint \"{src,scripts}/**/*.{js,ts}\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"scripts/**/*.js\"",
    "prepare": "husky",
    "prisma:migrate": "node scripts/migrate.js",
    "prisma:generate": "npx prisma generate"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.3.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.18.0",
    "prisma": "^7.4.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.22",
    "@prisma/internals": "^7.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.2.5",
    "@types/passport-local": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
